from random import random
from random import randint
liste=[]
liste2=[]  
def init_aleatoire(size):   
    for k in range(size):
        liste=[]
        for i in range(size):
            liste.append(randint(0,3))
        liste2.append(liste) 

    for i in range(size):
        print(liste2[i])
    return(liste2)
size=int(input("taille du monde "))

    

init_aleatoire(size)




